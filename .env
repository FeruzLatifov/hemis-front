# HEMIS Admin Panel Frontend - Environment Variables

# API Configuration
# Backend API URL (NEW-HEMIS Spring Boot)
VITE_API_URL=http://localhost:8081

# App Configuration
VITE_APP_NAME=HEMIS Admin Panel
VITE_APP_VERSION=1.0.0

# Default Locale
VITE_DEFAULT_LOCALE=uz

# ‚≠ê Sentry Error Tracking Configuration
# Enable/Disable Sentry (true/false, default: false)
# Development: DISABLED (set to true for production)
VITE_SENTRY_ENABLED=true

# Get your DSN from https://sentry.io/settings/projects/
# Leave empty for development
VITE_SENTRY_DSN=

# Environment (development, staging, production)
VITE_SENTRY_ENVIRONMENT=development

# Release version (for tracking which version caused errors)
VITE_SENTRY_RELEASE=hemis-front@2.0.0-dev

# Performance Monitoring - Traces Sample Rate (0.0 to 1.0)
VITE_SENTRY_TRACES_SAMPLE_RATE=0.2

# Session Replay - Sample Rate for normal sessions (0.0 to 1.0)
VITE_SENTRY_REPLAY_SESSION_SAMPLE_RATE=0.1

# Session Replay - Sample Rate for sessions with errors (0.0 to 1.0, usually 1.0)
VITE_SENTRY_REPLAY_ERROR_SAMPLE_RATE=1.0
